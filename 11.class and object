class Rect:
    def __init__(self, breadth, length):
        self.breadth = breadth
        self.length = length
    def area(self):
        return self.breadth * self.length
a = int(input("Enter breadth of rectangle: "))
n = int(input("Enter length of rectangle: "))
obj = Rect(a, n)
print("Area of rectangle:", obj.area())
